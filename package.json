{
  "name": "voting-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run migrate-db:dev && cross-env NODE_ENV=dev nodemon ./server/index.js",
    "serve": " vue-cli-service serve --fix",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "db:create": "cross-env NODE_ENV=dev node_modules/.bin/sequelize db:create",
    "db:drop": "cross-env NODE_ENV=dev node_modules/.bin/sequelize db:drop",
    "db:create:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:create",
    "db:drop:test": "cross-env NODE_ENV=test node_modules/.bin/sequelize db:drop",
    "migrate-db:dev": "cross-env NODE_ENV=dev sequelize db:migrate",
    "migrate-db:test": "cross-env NODE_ENV=test sequelize db:migrate",
    "migrate-db:dev:undo": "cross-env NODE_ENV=dev sequelize db:migrate:undo",
    "migrate-db:test:undo": "cross-env NODE_ENV=test sequelize db:migrate:undo",
    "start:dev:postgres": "cross-env NODE_ENV=dev npm run db:create && npm run migrate-db:dev",
    "start:test:postgres": "npm run db:create:test && npm run migrate-db:test",
    "test": "npm run migrate-db:test && cross-env NODE_ENV=test node_modules/.bin/mocha server/tests",
    "bump": "npm version patch --no-git-tag-version && git add package.json"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "forever": "^0.15.3",
    "helmet": "^3.13.0",
    "nodemon": "^1.18.3",
    "pg": "^7.4.3",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.11",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.11",
    "@vue/cli-service": "^3.0.0-rc.11",
    "babel-cli": "^6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "chai": "^4.1.2",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.17",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=8.11.3"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint --fix",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint --fix",
      "git add"
    ]
  },
  "pre-commit": [
    "bump"
  ]
}
